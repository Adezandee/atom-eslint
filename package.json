{
  "name": "fast-eslint",
  "version": "4.6.0",
  "description": "ESLint linter plugin for JavaScript, lightweight",
  "keywords": [
    "atom",
    "javascript",
    "linter",
    "lint",
    "eslint",
    "airbnb",
    "standard",
    "google",
    "xo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Adezandee/fast-eslint.git"
  },
  "homepage": "https://github.com/Adezandee/fast-eslint#readme",
  "author": "Arnaud Dezandee <dezandee.arnaud@gmail.com> (https://github.com/Adezandee)",
  "license": "MIT",
  "activationCommands": [],
  "engines": {
    "atom": ">=1.0.0"
  },
  "configSchema": {
    "baseConfigExtends": {
      "title": "BaseConfig Extends",
      "description": "List of ESLint shareable config names. See [shareable configs](http://eslint.org/docs/developer-guide/shareable-configs). Those will be used as *baseConfig.extends*. Example: **standard, standard-react**",
      "type": "array",
      "default": [],
      "items": {
        "type": "string"
      }
    },
    "grammarScopes": {
      "title": "Grammar scopes",
      "description": "Run `Editor: Log Cursor Scope` to determine file scopes.",
      "type": "array",
      "default": [
        "source.js",
        "source.jsx",
        "source.es6",
        "source.js.jsx",
        "source.babel",
        "source.js-semantic"
      ],
      "items": {
        "type": "string"
      }
    }
  },
  "main": "lib/index.js",
  "scripts": {
    "pretest": "eslint lib spec/*.js",
    "test": "apm test",
    "pregit:push": "git commit -am \"Prepare $npm_package_version release\"",
    "git:push": "git push",
    "apm:publish": "apm publish -t v$npm_package_version",
    "semantic-release": "semantic-release pre && npm run changelog && npm install && npm run git:push && semantic-release post && npm run apm:publish",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "atom-linter": "10.0.0",
    "atom-package-deps": "4.6.0",
    "eslint": "4.8.0",
    "loophole": "1.1.0"
  },
  "devDependencies": {
    "condition-circle": "~1.5.0",
    "conventional-changelog-cli": "~1.3.4",
    "cz-conventional-changelog": "~2.1.0",
    "eslint-config-airbnb-base": "~12.1.0",
    "eslint-plugin-import": "~2.7.0",
    "last-release-apm": "~1.3.1",
    "semantic-release": "~7.0.2"
  },
  "package-deps": [
    "linter:2.0.0"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "release": {
    "verifyConditions": "condition-circle",
    "getLastRelease": "last-release-apm"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
