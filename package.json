{
  "name": "fast-eslint",
  "version": "0.0.0-semantic-release",
  "description": "Lightweight zeroconf ESLint plugin for Atom.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Adezandee/fast-eslint.git"
  },
  "homepage": "https://github.com/Adezandee/fast-eslint#readme",
  "author": "Arnaud Dezandee <dezandee.arnaud@gmail.com> (https://github.com/Adezandee)",
  "license": "MIT",
  "activationCommands": [],
  "engines": {
    "atom": ">=1.0.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "apm test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "dependencies": {
    "atom-linter": "^4.1.1",
    "atom-package-deps": "^3.0.6",
    "eslint": "^1.10.3",
    "lodash.uniq": "^3.2.2",
    "loophole": "^1.1.0"
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "devDependencies": {
    "babel-eslint": "^4.1.5",
    "condition-circle": "^1.2.0",
    "cz-conventional-changelog": "^1.1.5",
    "eslint-config-airbnb": "^2.1.1",
    "semantic-release": "^4.3.5"
  },
  "release": {
    "verifyConditions": "condition-circle"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
