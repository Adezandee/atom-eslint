{
  "name": "fast-eslint",
  "version": "1.1.5",
  "description": "Lightweight zeroconf ESLint plugin for Atom.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Adezandee/fast-eslint.git"
  },
  "homepage": "https://github.com/Adezandee/fast-eslint#readme",
  "author": "Arnaud Dezandee <dezandee.arnaud@gmail.com> (https://github.com/Adezandee)",
  "license": "MIT",
  "activationCommands": [],
  "engines": {
    "atom": ">=1.0.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "pretest": "eslint .",
    "test": "apm test",
    "git:push": "git commit -am \"Prepare v$npm_package_version release [ci skip]\"",
    "postgit:push": "git push",
    "apm:publish": "apm publish -t v$npm_package_version",
    "semantic-release": "semantic-release pre && npm run git:push && semantic-release post && npm run apm:publish"
  },
  "dependencies": {
    "atom-linter": "^4.3.2",
    "atom-package-deps": "^3.0.6",
    "eslint": "^1.10.3",
    "lodash.uniq": "^4.0.1",
    "loophole": "^1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "condition-circle": "^1.2.0",
    "cz-conventional-changelog": "^1.1.5",
    "eslint-config-airbnb": "^3.1.0",
    "last-release-apm": "^1.0.0",
    "semantic-release": "^4.3.5"
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "release": {
    "verifyConditions": "condition-circle",
    "getLastRelease": "last-release-apm"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}