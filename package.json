{
  "name": "fast-eslint",
  "version": "2.15.1",
  "description": "ESLint linter plugin for JavaScript, lightweight zeroconf",
  "keywords": [
    "atom",
    "javascript",
    "linter",
    "lint",
    "eslint",
    "airbnb",
    "standard",
    "google",
    "xo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Adezandee/fast-eslint.git"
  },
  "homepage": "https://github.com/Adezandee/fast-eslint#readme",
  "author": "Arnaud Dezandee <dezandee.arnaud@gmail.com> (https://github.com/Adezandee)",
  "license": "MIT",
  "activationCommands": [],
  "engines": {
    "atom": ">=1.0.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "pretest": "eslint lib",
    "test": "apm test",
    "pregit:push": "git commit -am \"Prepare $npm_package_version release\"",
    "git:push": "git push",
    "apm:publish": "apm publish -t v$npm_package_version",
    "semantic-release": "semantic-release pre && npm run changelog && npm run git:push && semantic-release post && npm run apm:publish",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "atom-linter": "~6.0.0",
    "atom-package-deps": "~4.0.1",
    "babel-eslint": "~6.1.0",
    "eslint": "~2.13.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-config-airbnb-base": "3.0.1",
    "eslint-config-google": "0.6.0",
    "eslint-config-standard": "5.3.3",
    "eslint-config-standard-jsx": "1.2.1",
    "eslint-config-xo": "0.15.2",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-import": "1.10.2",
    "eslint-plugin-jsx-a11y": "1.5.5",
    "eslint-plugin-promise": "1.3.2",
    "eslint-plugin-react": "5.2.2",
    "eslint-plugin-standard": "1.3.2",
    "lodash.uniq": "~4.3.0",
    "loophole": "~1.1.0"
  },
  "devDependencies": {
    "condition-circle": "^1.2.0",
    "conventional-changelog-cli": "^1.2.0",
    "cz-conventional-changelog": "^1.1.6",
    "last-release-apm": "^1.2.1",
    "semantic-release": "^4.3.5"
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "release": {
    "verifyConditions": "condition-circle",
    "getLastRelease": "last-release-apm"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}