{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "globals": {
    "atom": true
  },
  "rules": {
    "no-console": 1,
    "comma-dangle": 0,
    "no-param-reassign": 0,
    "max-len": 0
  }
}
